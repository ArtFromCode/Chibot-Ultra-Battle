Sub mSaveChar_Click ()
Dim S As String
  If ChecksOkay(Senshi(1)) Then
    On Error GoTo DontSave1
    CmdChr.Filename = First8(Senshi(Num).SenshiID) + ".ch2"
    CmdChr.DialogTitle = "Save Character"
    CmdChr.FilterIndex = 2
    CmdChr.Action = 2
    S = CmdChr.Filename
    If Len(S) > 4 Then
      If UCase(Right$(S, 3) = "CHR") Then
        SaveChar S, Num
      Else
        SaveNewChar S, Num
      End If
    Else
      SaveNewChar S, Num
    End If
    Changed = False
    Temp1 = 0
    InfoLine = "Character " + S + " successfully saved."
DontSave1:
    On Error GoTo 0
    Exit Sub
  Else
    Temp1 = 1
  End If
End Sub

